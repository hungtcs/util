{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "Bundler",
    "baseUrl": "./",
    "jsx": "preserve",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.dumi/*"],
      "rc-util": ["src/index.tsx"],
      "rc-util/es/*": ["src/*"]
    }
  }
}
